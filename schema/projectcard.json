{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Project Card Schema",
	"required": [
		"project",
		"changes"
	],
	"properties": {
        "project": {"type": "string","description": "A project name which uniquely identifies this project"},
		"changes": {
			"type": "array",
			"items": {
				"oneOf": [
					{ "$ref": "defs/change.roadway.delete.json"},
					{ "$ref": "defs/change.roadway.add.json"},
					{ "$ref": "defs/change.roadway.property-changes.json"},
					{ "$ref": "defs/change.roadway.parallel-managed-lanes.json"}
			  ]
			}
		},
		"notes": {"type": "string"}
	}
}
