{
	"$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
	"required": [
		"category",
		"facility",
		"properties"
	],
	"definitions": {
		"lanes.change-item":{
			"type":"object",
			"required":["property","set"],
			"properties":{
				"property":{"type":"string","value":"lanes"},
				"set" : {"type": ["number", "string"]}
			}
		},
		"ML_lanes.change-item":{
			"type":"object",
			"required":["property","set"],
			"properties":{
				"property":{"type":"string","value":"ML_lanes"},
				"set" : {"type": ["number", "string"]}

			}
		}
	},
	"additionalProperties" : false,
	"properties": {
		"category": {
			"type": "string",
			"pattern": "parallel managed lanes"
	   	},
		"facility": {
			"type": "object",
		 	"$ref": "roadway.facility.json"
   		},
		"properties": {
			"type":"array",
			"contains":{"allOf": [
				{"$ref":"#/definitions/lanes.change-item"},
				{"$ref":"#/definitions/ML_lanes.change-item"}
			]}
		}
	}
}
