{
    "$schema": "http://json-schema.org/draft-07/schema",
    "id": "roadway.property-changes",
    "type": "array",
    "definitions":{
        "set": {
            "id": "property.set",
            "type" : "object",
            "required": ["property","set"],
            "properties" : {
                "property" : {"type" : "string"},
                "existing" : {"type": ["number", "string"]},
                "set" : {"type": ["number", "string"]}
            }
        },
        "existing-change": {
            "id": "property.existing-change",
            "type" : "object",
            "required": ["property","existing","change"],
            "properties" : {
                "property" : {"type" : "string"},
                "existing" : {"type" : "number"},
                "change" : {"type" : "number"}
            }
        }
    },
    "items" : {
        "oneOf" : [ 
            {"$ref": "#/definitions/set"},
            {"$ref": "#/definitions/existing-change"}
        ]
    }
}
