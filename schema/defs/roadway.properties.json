{
    "$schema": "http://json-schema.org/draft-07/schema",
    "id": "roadway.properties",
    "type": "array",
    "items" : {
        "oneOf" : [
            {
                "type" : "object",
                "required": ["property","set"],
                "properties" : {
                    "property" : {"type" : "string"},
                    "existing" : {"type": ["number", "string"]},
                    "set" : {"type": ["number", "string"]}
                }
            },
            {
                "type" : "object",
                "required": ["property","existing","change"],
                "properties" : {
                    "property" : {"type" : "string"},
                    "existing" : {"type" : "number"},
                    "change" : {"type" : "number"}
                }
            }
        ]
    }
}
